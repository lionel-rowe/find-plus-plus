{
	"manifest_version": 3,
	"name": "Find++",
	"description": "Enhanced find-in-page, with regexes!",
	"version": "0.0.0",
	"icons": {
	},
	"content_scripts": [
		{
			"js": ["./main.js"],
			"matches": ["<all_urls>"],
			"world": "MAIN"
		}
	],
	"web_accessible_resources": [
		{
			"resources": ["/template.html"],
			"matches": ["<all_urls>"]
		}
	],
	"permissions": [
		"activeTab",
		"management"
	],
	"background": {
		"service_worker": "background.js"
	}
}
